{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Your own server",
            "request": "launch",
            "type": "al",
            "environmentType": "OnPrem",
            "server": "http://localHost:8080/ROTICH2",
            "serverInstance": "ROTICH2",
            "authentication": "Windows",
            "startupObjectId": 60102,
            "startupObjectType": "Page",
            "breakOnError": "All",
            "launchBrowser": true,
            "port": 4564,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "tenant": "default",
            "schemaUpdateMode": "ForceSync"
        },
        {
            "name": "Rotich Debugger",
            "request": "attach",
            "type": "al",
            "environmentType": "OnPrem",
            "server": "http://localHost:8080/ROTICH2",
            "serverInstance": "ROTICH2",
            "authentication": "Windows",          
            "port": 4564,
            "breakOnError": true,
            "breakOnRecordWrite": false,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "breakOnNext": "WebClient",
        },
        {
            "name": "Rotich Attach",
            "request": "attach",
            "type": "al",
            "server": "http://localhost",
            "serverInstance": "ROTICH2",
            "authentication": "Windows",          
            "port": 4564,
            "breakOnError": true,
            "breakOnRecordWrite": false,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "breakOnNext": "WebClient",
        }
    ]
}